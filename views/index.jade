					li.lower-social__item
						a.lower-social__link.icon-twitter(href="#")
					li.lower-social__item
						a.lower-social__link.icon-youtube(href="#")
					li.lower-social__item
						a.lower-social__link.icon-instagram(href="#")

				ul.legal-navigation
