	body
		header.main-header
			div.basic-container
				h1.main-page-title
					a.main-page-title__link.icon-fuse-logo(href="#")
						span.path1
						span.path2
						span.path3
						span.path4
						span.path5
						span.path6
						span.path7
						span.path8

				nav.main-navigation-container
					ul.main-navigation
						li.main-navigation__item
							a.main-navigation__link(href='#') Shows
						li.main-navigation__item
							a.main-navigation__link(href='#') Festivals
						li.main-navigation__item
							a.main-navigation__link(href='#') Fun Stuff
						li.main-navigation__item
							a.main-navigation__link(href='#') Music
					ul.dont-miss
						li.dont-miss__item
							h2.dont-miss__title Don't miss:
						li.dont-miss__item
							a.dont-miss__link(href='#') Legends Football
						li.dont-miss__item
							a.dont-miss__link(href='#') Leage
						li.dont-miss__item
							a.dont-miss__link(href='#') SKEE TV
						li.dont-miss__item
							a.dont-miss__link(href='#') Got Ur #
				div.global-search.icon-search
					input.global-search__search-bar(type="text", id="fuse-search-field", name="q", placeholder="Search")

				ul.upper-social
					li.upper-social__item
						a.upper-social__link.icon-facebook(href="#")
					li.upper-social__item
						a.upper-social__link.icon-twitter(href="#")
					li.upper-social__item
						a.upper-social__link.icon-youtube(href="#")
					li.upper-social__item
						a.upper-social__link.icon-instagram(href="#")
					li.upper-social__item
						a.upper-social__link.icon-fm(href="#")

		
		div.basic-container
			img.ad__leaderboard(src='themes/default/images/mock_leaderboard_ad.png')
				
		div.basic-container.basic-container__box-shadow
			article.gallery__container
				h2.gallery__rubric FATHER'S DAY
				h2.gallery__title PHOTO GALLERY: 17 MUSICAL DADS AND THEIR KIDS
				p.gallery__description Just in time for Father's Day, take a look through our gallery featuring Kanye West, Nick Lachey, and 16 other musicians doing that other thing they do—being cool dads!
					
				div.gallery__main-column
					mixin publishDateContributor()
					mixin listControls()
					div.gallery__slides-container
						.gallery__slide#1
							img(src='themes/default/images/gallery_image.png').gallery__image
						.gallery_image-credit @michaelbuble on Instagram
				div.gallery__right-rail
					mixin socialShare()
					h2.gallery__slide-title User
					p.gallery__slide-description Do you think 'The Voice' coach is talking strategy with sons Usher and Naviyd? Or just doodling?
				div.ad__300x250
					div.ad__image
					span.ad__text-bottom Advertisement
			section.main-column
			div.right-rail


		footer.main-footer
			div.basic-container
				h1.main-page-title
					a.main-page-title__link.icon-fuse-logo(href="#")
						span.path1
						span.path2
						span.path3
						span.path4
						span.path5
						span.path6
						span.path7
						span.path8

